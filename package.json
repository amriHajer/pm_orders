{
  "name": "analyse_cout_maintenance",
  "version": "0.0.1",
  "description": "An SAP Fiori application pour analyse des  co√ªts de  maintenance .",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "devDependencies": {
    "@sap-ux/ui5-middleware-fe-mockserver": "^2.3.9",
    "@sap/ux-ui5-tooling": "^1.19.0",
    "@ui5/cli": "^3.11.10",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "fiori run --open \"test/flpSandbox.html?sap-client=110&sap-ui-xx-viewCache=false#acmanalysecoutmaintenance-tile\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandbox.html?sap-client=110&sap-ui-xx-viewCache=false#acmanalysecoutmaintenance-tile\"",
    "build": "ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "start-mock": "fiori run --config ./ui5-mock.yaml --open \"test/flpSandbox.html?sap-client=110&sap-ui-xx-viewCache=false#acmanalysecoutmaintenance-tile\"",
    "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml && rimraf archive.zip",
    "deploy-config": "fiori add deploy-config",
    "start-noflp": "fiori run --open \"index.html?sap-client=110&sap-ui-xx-viewCache=false\"",
    "start-variants-management": "fiori run --open \"preview.html?&sap-client=110&sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
    "undeploy": "npm run build && fiori undeploy --config ui5-deploy.yaml",
    "deploy-test": "npm run build && fiori deploy --config ui5-deploy.yaml --testMode true"
  },
  "sapuxLayer": "CUSTOMER_BASE",
  "sapux": true
}
